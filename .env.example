# Para Docker
DATABASE_URL= "postgresql://postgres:postgres@db:5432/titulos?schema=public"

#Porta da Aplicação
APP_PORT=5500

#Porta do DataBase
DB_PORT=5432

#Login Usuário Administrativo, CPF aleatório valido.
USER_ADMIN= 39618293653

#Senha Usuário Administrativo
